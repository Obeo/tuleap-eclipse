<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Tuleap" location="/org.tuleap.mylyn.task-feature/codeStyle/CheckstyleConfiguration.xml" type="project" description="Tuleap Checkstyle Configuration">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <local-check-config name="Tuleap Tests" location="/org.tuleap.mylyn.task-feature/codeStyle/CheckstyleTestConfiguration.xml" type="project" description="The checkstyle configuration of the tests project.">
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Tuleap Tests" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
