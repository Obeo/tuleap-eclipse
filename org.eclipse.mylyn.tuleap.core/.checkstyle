<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Tuleap" location="/org.eclipse.mylyn.tuleap-feature/codeStyle/CheckstyleConfiguration.xml" type="project" description="Tuleap Checkstyle Configuration">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Tuleap" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src-gen"/>
  </filter>
</fileset-config>
