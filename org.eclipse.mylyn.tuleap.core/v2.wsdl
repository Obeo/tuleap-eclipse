tion name="updateArtifact"><soap:operation soapAction="https://demo.tuleap.net/plugins/tracker/soap#updateArtifact" style="rpc"/><input><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></input><output><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></output></operation><operation name="getArtifact"><soap:operation soapAction="https://demo.tuleap.net/plugins/tracker/soap#getArtifact" style="rpc"/><input><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></input><output><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></output></operation><operation name="getTrackerStructure"><soap:operation soapAction="https://demo.tuleap.net/plugins/tracker/soap#getTrackerStructure" style="rpc"/><input><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></input><output><soap:body use="encoded" namespace="https://demo.tuleap.net/plugins/tracker/soap" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/></output></operation></binding>
